# This file is for unifying the coding style for different editors and IDEs
# editorconfig.org

root = true

[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
indent_style = space
indent_size = 4

[**.json]
indent_size = 2

[**.scss]
indent_size = 2

[**.js]
indent_size = 2

[**.css]
indent_size = 2
